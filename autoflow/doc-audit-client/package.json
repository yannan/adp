{
  "name": "doc-audit-client",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "serve": "cross-env NODE_OPTIONS=--openssl-legacy-provider vue-cli-service serve",
    "build": "cross-env NODE_OPTIONS=--openssl-legacy-provider vue-cli-service build",
    "build:uat": "cross-env NODE_OPTIONS=--openssl-legacy-provider vue-cli-service build --mode uat",
    "build:dev": "cross-env NODE_OPTIONS=--openssl-legacy-provider vue-cli-service build --mode development",
    "build:anyshare-200": "cross-env NODE_OPTIONS=--openssl-legacy-provider vue-cli-service build --mode anyshare-200",
    "build:anyshare-45": "cross-env NODE_OPTIONS=--openssl-legacy-provider vue-cli-service build --mode anyshare-45",
    "build:anyshare-171": "cross-env NODE_OPTIONS=--openssl-legacy-provider vue-cli-service build --mode anyshare-171",
    "build:production": "cross-env NODE_OPTIONS=--openssl-legacy-provider vue-cli-service build --mode production",
    "build:lib": "cross-env NODE_OPTIONS=--openssl-legacy-provider vue-cli-service build --mode anyshare-171 --target lib --name auditClient src/Build.vue --inline-vue",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit",
    "test": "jest --config=../../config/jest.config.js --passWithNoTests"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "copy-to-clipboard": "^3.3.3",
    "element-ui": "^2.9.2",
    "file-saver": "^2.0.5",
    "jquery": "^3.4.1",
    "js-base64": "^2.6.4",
    "js-cookie": "^3.0.1",
    "jszip": "^3.6.0",
    "qiankun": "^2.8.3",
    "sass-loader": "^7.1.0",
    "v-tooltip": "^2.1.3",
    "vue": "^2.7.16",
    "vue-i18n": "^8.22.4",
    "vue-router": "3.0.6",
    "vuedraggable": "^2.24.3",
    "vuex": "^3.1.0",
    "vxe-utils": "^1.7.2",
    "xe-utils": "^2.2.2",
    "webpack": "4.47.0",
    "webpack-dev-server": "3.11.3",
    "webpack-cli": "3.3.12"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "3.5.3",
    "@vue/cli-plugin-eslint": "3.5.1",
    "@vue/cli-service": "3.5.3",
    "babel-eslint": "10.0.1",
    "babel-plugin-transform-remove-strict-mode": "^0.0.2",
    "eslint": "6.7.2",
    "eslint-plugin-vue": "6.2.2",
    "html-webpack-plugin": "3.2.0",
    "script-ext-html-webpack-plugin": "2.1.3",
    "svg-sprite-loader": "^6.0.11",
    "terser-webpack-plugin": "1.4.5",
    "vue-template-compiler": "^2.7.16",
    "cross-env": "^7.0.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
