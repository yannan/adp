<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.eisoo.metadatamanage.db.mapper.LineageEdgeColumnTableRelationMapper">

    <!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.eisoo.metadatamanage.db.entity.LineageEdgeColumnTableRelationEntity" id="baseMap">
        <result property="id" column="f_id"/>
        <result property="tableId" column="f_table_id"/>
        <result property="columnId" column="f_column_id"/>
    </resultMap>

    <insert id="batchSave">
        insert into t_lineage_edge_column_table_relation (f_id,f_table_id,f_column_id)
        values
        <foreach collection="list" item="item" separator=",">
            (
            #{item.id},
            #{item.tableId},
            #{item.columnId}
            )
        </foreach>
    </insert>


</mapper>